      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{15C6B181-83AD-4457-A2FB-1A59C47F6D90}"
          SQLTask:SqlStatementSource="DECLARE @Var1 decimal(11,10), @Var2 decimal(11,10)&#xA;&#xA;SELECT @Var1 = RiskThreshold FROM Ref_RiskLevel WHERE RiskLevelID = 1&#xA;SELECT @Var2 = RiskThreshold FROM Ref_RiskLevel WHERE RiskLevelID = 2&#xA;&#xA;UPDATE Patient&#xA;SET RiskLevel = &#xA;  CASE&#xA;    when RiskScore &gt;= @var1 then 1&#xA;    when RiskScore &gt;= @Var1 and RiskScore &lt; @Var2 then 2&#xA;    else NULL&#xA;  END&#xA;WHERE ScoringStatus &lt;&gt; 2&#xA;&#xA;UPDATE Patient&#xA;SET DateIdentifiedAsAtRisk = cast(getdate() as date)&#xA;WHERE RiskLevel is not NULL&#xA;AND DateIdentifiedAsAtRisk = NULL&#xA;AND ScoringStatus &lt;&gt; 2" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
